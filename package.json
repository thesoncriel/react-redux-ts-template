{
  "name": "react-redux-ts-template",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "start": "npm run env:dev && react-scripts start",
    "start:test": "npm run env:test && react-scripts start",
    "start:prod": "npm run env:prod && react-scripts start",
    "build": "react-scripts build",
    "build:test": "npm run env:test && react-scripts build",
    "build:prod": "npm run env:prod && react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "env:dev": "cp ./environments/.env.dev ./.env",
    "env:test": "cp ./environments/.env.test ./.env",
    "env:prod": "cp ./environments/.env.production ./.env",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "sprite": "gulp makesprite"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
